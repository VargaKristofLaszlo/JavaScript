<%- include('TableHeader.ejs') -%>
<%- include('Navbar.ejs') -%>
<table class="tableStyle">
    <thead>
    <tr>
        <th scope="col">Name</th>
        <th scope="col">Phone number</th>
        <th scope="col">E-mail</th>
        <th scope="col">Medical specialty</th>
        <th scope="col">&nbsp</th>
    </tr>
    </thead>
    <tbody>
    <%  doctorData.forEach(function (doctor){ %>
        <tr>
            <td><%= doctor.Name%>   </td>
            <td><%= doctor.Phone_number%>   </td>
            <td><%= doctor.E_mail%> </td>
            <td><%= doctor.Medical_scpecialty%> </td>
            <td>
                <a href='/Doctors/edit/<%= doctor._DoctorID%>'> <i class="far fa-edit"> </i> </a>
                <a href='#'> <i class="far fa-trash-alt"></i> </a>
            </td>
        </tr>

    <%  });  %>

    </tbody>
</table>
<%- include('Footer.ejs') -%>